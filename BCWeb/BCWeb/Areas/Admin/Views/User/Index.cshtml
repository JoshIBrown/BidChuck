@{
    ViewBag.Title = "User Profiles";
    Layout = "~/Views/Shared/_Admin.cshtml";
}

<div class="row">
    <div class="large-4 columns">
        <h4>@ViewBag.Title</h4>
    </div>
</div>
<div class="row">
    <div class="large-12 columns">
        <ul></ul>
    </div>
</div>
<div class="row">
    <div class="large-12 columns" ng-controller="UserProfileCtrl">
        <table ng-datatable="options" aa-data="theData" ao-column-defs="columnDefs">
            <thead>
                <tr>
                    <th>Id</th>
                    <th>Email</th>
                    <th>Last Name</th>
                    <th>First Name</th>
                    <th>Job Title</th>
                    <th>Company Id</th>
                    <th>Confirmed</th>
                </tr>
            </thead>
        </table>
    </div>
</div>



@section scripts{
    @Scripts.Render("~/bundles/admin/userProfile")
}
@section head{
    @Styles.Render("~/Content/DataTables/media/css/jquery.dataTables.css")
}